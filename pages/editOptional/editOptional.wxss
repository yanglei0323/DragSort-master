}
.move_row{
	z-index:888;
}

.mua_box{
	width:156rpx;
	height:156rpx;
	position:fixed;
	left:0;
	bottom:0;
	z-index:10000;
	overflow-x:hidden;
	-webkit-transition: width 0.3s;
  	transition: width 0.3s;
}
.mua_box image{
	width:156rpx;
